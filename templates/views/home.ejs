            <li class="nav-item mr-2 mb-3 mb-lg-0">
              <a class="btn btn-secondary btn-sm" href="/moderator">Dashboard</a>
            </li>
            <% }else{ %> 
              <li class="nav-item mr-2 mb-3 mb-lg-0">
                <a class="btn btn-secondary btn-sm" href="/<%=usertype %> ">Dashboard</a>
              </li>
              <% } %> 
